<h1>Master Variants</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @master_variants.each do |master_variant| %>
      <tr>
      <td><%= master_variant.variant%></td>
        <td><%= link_to 'Show', master_variant %></td>
        <td><%= link_to 'Edit', edit_master_variant_path(master_variant) %></td>
        <td><%= link_to 'Destroy', master_variant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Variant', new_master_variant_path %>
